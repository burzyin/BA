<script src="http://open.mapquestapi.com/sdk/js/v7.0.s/mqa.toolkit.js"></script>
<script charset="utf-8" src="${request.getContextPath()}/js/maps/openStreetMap/control.js"></script>
<script>

	var mapSettings = $.parseJSON($properties.toJsString());
    var urls = {
        sightsRequest: '${request.getContextPath()}' + '/attractions' + '$sights'
    };

    /*The MQA.EventUtil hooks into the window load event and execute defined function
     passed in as the last parameter.*/
    MQA.EventUtil.observe(window, 'load', function () {
        initialize('mapWrapper', mapSettings, urls);
    });
</script>

#set($page_title = "Belarus Attraction: OpenStreetMap")

<div id="mapWrapper" style="width: 900px; height: 700px;"></div>